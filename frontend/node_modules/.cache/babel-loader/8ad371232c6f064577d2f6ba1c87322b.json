k_image}`\" style=\"height: 70px\"\r\n\t\t\t\t\t\t\t\t\t\t\t\talt=\"image\" />\r\n\t\t\t\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t\t\t\t\t<img v-else src=\"/assets/images/product/1.png\" alt=\"image\" />\r\n\t\t\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t\t\t\t<td>{{ entry.book_name }}</td>\r\n\t\t\t\t\t\t\t\t\t<td>{{ entry.user_name }}</td>\r\n\t\t\t\t\t\t\t\t\t<td>{{ entry.time }}</td>\r\n\t\t\t\t\t\t\t\t\t<td>{{ entry.date }}</td>\r\n\t\t\t\t\t\t\t\t\t<td>{{ entry.return_date }}</td>\r\n\t\t\t\t\t\t\t\t\t<td>\r\n\t\t\t\t\t\t\t\t\t\t<div class=\"d-flex justify-content-end\">\r\n\t\t\t\t\t\t\t\t\t\t\t<a href=\"#\" v-if=\"entry.book_return\"\r\n\t\t\t\t\t\t\t\t\t\t\t\tclass=\"btn btn-sm btn-success mx-1\">Returned</a>\r\n\t\t\t\t\t\t\t\t\t\t\t<a href=\"#\" @click.prevent=\"return_book(entry)\" v-else\r\n\t\t\t\t\t\t\t\t\t\t\t\tclass=\"btn btn-sm btn-danger mx-1\">Return</a>\r\n\r\n\t\t\t\t\t\t\t\t\t\t\t<router-link :to=\"{ name: 'studentEntryDetails', params: { id: entry.id } }\"\r\n\t\t\t\t\t\t\t\t\t\t\t\tclass=\"btn btn-sm btn-primary mx-1\">Show Details</router-link>\r\n\t\t\t\t\t\t\t\t\t\t</div>\r\n\t\t\t\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t\t\t</tr>\r\n\t\t\t\t\t\t\t</tbody>\r\n\t\t\t\t\t\t</table>\r\n\t\t\t\t\t</div>\r\n\t\t\t\t\t<div class=\"card-footer\">\r\n\t\t\t\t\t\t<pagination v-model=\"page\" :options=\"pagination_option\" :records=\"total\" :per-page=\"per_page\"\r\n\t\t\t\t\t\t\t@paginate=\"getData\" />\r\n\t\t\t\t\t</div>\r\n\t\t\t\t</div>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport {\r\n\t\tmapGetters\r\n\t} from \"vuex\";\r\n\texport default {\r\n\t\tname: \"bookList\",\r\n\t\tdata: function () {\r\n\t\t\treturn {\r\n\t\t\t\tbook_entries: {},\r\n\t\t\t\tpage: 1,\r\n\t\t\t\tper_page: 0,\r\n\t\t\t\ttotal: 0,\r\n\t\t\t\tpagination_option: {\r\n\t\t\t\t\tedgeNavigation: true,\r\n\t\t\t\t},\r\n\t\t\t};\r\n\t\t},\r\n\t\tcreated: function () {\r\n\t\t\tthis.getData();\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\tgetData: function (page = 1) {\r\n\t\t\t\tlet url = `/book-entry/user-entries?page= ${page}`;\r\n\t\t\t\twindow.axios.get(url).then((res) => {\r\n\t\t\t\t\tconsole.log(res.data);\r\n\t\t\t\t\tthis.book_entries = res.data;\r\n\t\t\t\t\tthis.total = res.data.total;\r\n\t\t\t\t\tthis.per_page = res.data.per_page;\r\n\t\t\t\t});\r\n\t\t\t},\r\n\t\t\treturn_book: function (entry) {\r\n\t\t\t\tlet con = confirm('sure');\r\n\t\t\t\tcon &&\r\n\t\t\t\twindow.axios.post(`/book-entry/return-book`, {id: entry.id})\r\n\t\t\t\t.then((res) => {\r\n\t\t\t\t\tconsole.log(res.data);\r\n\t\t\t\t\tentry.book_return = 1;\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t},\r\n\t\tcomputed: {\r\n\t\t\t...mapGetters([\"get_server_url\"]),\r\n\t\t},\r\n\t};\r\n</script>\r\n\r\n<style></style>"]}]}