v class=\"info\">\r\n          <div class=\"row\">\r\n            <div class=\"col-sm-6 col-lg-4 order-sm-1 order-xl-0\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-6\">\r\n                  <div class=\"ttl-info text-left\">\r\n                    <h6>\r\n                      <i class=\"fa fa-envelope\"></i>&nbsp;&nbsp;&nbsp;Email\r\n                    </h6>\r\n                    <span>{{ get_auth_info.email }}</span>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-md-6\">\r\n                  <div class=\"ttl-info text-left\">\r\n                    <h6><i class=\"fa fa-calendar\"></i>&nbsp;&nbsp;&nbsp;BOD</h6>\r\n                    <span>06 December 1991</span>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-sm-12 col-lg-4 order-sm-0 order-xl-1\">\r\n              <div class=\"user-designation\">\r\n                <div class=\"title\">\r\n                  <a target=\"_blank\" href=\"#\">{{ get_auth_info.username }}</a>\r\n                </div>\r\n                <div class=\"desc mt-2\">\r\n                  {{ get_auth_info.role_name }}\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <div class=\"col-sm-6 col-lg-4 order-sm-2 order-xl-2\">\r\n              <div class=\"row\">\r\n                <div class=\"col-md-6\">\r\n                  <div class=\"ttl-info text-left\">\r\n                    <h6>\r\n                      <i class=\"fa fa-phone\"></i>&nbsp;&nbsp;&nbsp;Contact Us\r\n                    </h6>\r\n                    <span>BD +880 179-581-5660</span>\r\n                  </div>\r\n                </div>\r\n                <div class=\"col-md-6\">\r\n                  <div class=\"ttl-info text-left\">\r\n                    <h6>\r\n                      <i class=\"fa fa-location-arrow\"></i\r\n                      >&nbsp;&nbsp;&nbsp;Location\r\n                    </h6>\r\n                    <span>SBG Schoool Demo Home</span>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <hr />\r\n          <div class=\"social-media\">\r\n            <ul class=\"list-inline\">\r\n              <li class=\"list-inline-item\">\r\n                <a href=\"#\"><i class=\"fa fa-facebook\"></i></a>\r\n              </li>\r\n              <li class=\"list-inline-item\">\r\n                <a href=\"#\"><i class=\"fa fa-google-plus\"></i></a>\r\n              </li>\r\n              <li class=\"list-inline-item\">\r\n                <a href=\"#\"><i class=\"fa fa-twitter\"></i></a>\r\n              </li>\r\n              <li class=\"list-inline-item\">\r\n                <a href=\"#\"><i class=\"fa fa-instagram\"></i></a>\r\n              </li>\r\n              <li class=\"list-inline-item\">\r\n                <a href=\"#\"><i class=\"fa fa-rss\"></i></a>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n          <div class=\"follow\">\r\n            <div class=\"row\">\r\n              <div class=\"col-6 text-md-right border-right\">\r\n                <div class=\"follow-num counter\">564.505</div>\r\n                <span>Follower</span>\r\n              </div>\r\n              <div class=\"col-6 text-md-left\">\r\n                <div class=\"follow-num counter\">0.9531805</div>\r\n                <span>Following</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"row card mt-4\">\r\n            <div class=\"col-12 card-body\">\r\n              <form\r\n                action=\"#\"\r\n                id=\"update_profile\"\r\n                @submit.prevent=\"update_profile\"\r\n              >\r\n                <h3>Update Profile</h3>\r\n                <div class=\"form-group text-left\">\r\n                  <label for=\"\">Username</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"name\"\r\n                    :value=\"get_auth_info.username\"\r\n                    class=\"form-control\"\r\n                  />\r\n                </div>\r\n                <div class=\"form-group text-left\">\r\n                  <label for=\"\">Password</label>\r\n                  <input type=\"text\" name=\"password\" class=\"form-control\" />\r\n                </div>\r\n                <div class=\"form-group text-left\">\r\n                  <label for=\"\">Confirm Password</label>\r\n                  <input\r\n                    type=\"text\"\r\n                    name=\"password_confirmation\"\r\n                    class=\"form-control\"\r\n                  />\r\n                </div>\r\n                <div class=\"form-group text-left\">\r\n                  <button class=\"btn btn-info\">Submit</button>\r\n                </div>\r\n              </form>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapMutations } from \"vuex\";\r\nexport default {\r\n  name: \"profile\",\r\n  getData: function () {},\r\n  created: function () {\r\n    console.log(this.get_auth_info);\r\n  },\r\n  methods: {\r\n    ...mapMutations([\"set_auth_info\"]),\r\n    update_profile: function () {\r\n      let form_data = new FormData(document.getElementById(\"update_profile\"));\r\n      window.axios.defaults.headers.common[\r\n        \"Authorization\"\r\n      ] = `Bearer ${this.get_auth_token}`;\r\n      window.axios.post(\"/user/update-profile\", form_data).then((res) => {\r\n        console.log(res.data);\r\n        this.set_auth_info(res.data.user);\r\n      });\r\n    },\r\n\r\n    change_profile: function () {\r\n      let form_data = new FormData(\r\n        document.getElementById(\"change_profile_form\")\r\n      );\r\n      window.axios.defaults.headers.common[\r\n        \"Authorization\"\r\n      ] = `Bearer ${this.get_auth_token}`;\r\n      window.axios.post(\"/user/update-profile-pic\", form_data).then((res) => {\r\n        // console.log(res.data);\r\n        this.set_auth_info(res.data.user);\r\n      });\r\n    },\r\n  },\r\n  computed: {\r\n    ...mapGetters([\"get_auth_info\", \"get_auth_token\", \"get_profile_image_url\"]),\r\n  },\r\n};\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.user-profile .hovercard .cardheader {\r\n    background: url(/assets/images/auth-bg.jpg);\r\n    background-size: cover;\r\n    background-position: 10%;\r\n    height: 470px;\r\n}\r\n</style>\r\n"]}]}